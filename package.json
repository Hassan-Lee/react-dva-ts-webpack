{
  "name": "supngin",
  "version": "1.0.0",
  "description": "数据中台",
  "main": "index.ts",
  "scripts": {
    "precommit": "npm run eslint && lint-staged",
    "eslint": "eslint --ext .tsx,.ts --fix ./src",
    "test": "jest",
    "add": "add-component",
    "lint-staged": "lint-staged",
    "lint": "eslint src --ext .ts,.tsx && stylelint \"./src/**/*.less\"",
    "dev": "cross-env NODE_ENV=development APP_ENV=dev webpack-dev-server --colors --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "build:qa": "cross-env APP_ENV=qa npm run build",
    "build:prod": "cross-env APP_ENV=prod npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,less,css}": [
      "prettier --write",
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": ["webpack", "typescript", "admin", "react", "dva"],
  "author": "supngin",
  "license": "MIT",
